@using AlbertLlonguerasNew.Models
@using Library.Businness
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "BasePage.cshtml";
}


@section bodyContent
{
    <div class="columnsContainer">
 
      <div class="leftColumn">
          <h2>Nou partit</h2>
    @{
        var paco = Umbraco.TypedContent(1151);
        var paco2 = paco.Descendants("Jugador");
        TempData["node"] = paco2;
    }
    @Html.Partial("CreateResult", new NewResultModel
{
    Player = "master",
    PorraNode = PorraManager.GetMatchNode(Model.Content),
    MatchIdentifier = PorraManager.GetMatchIdentifier(Model.Content),
    PlayerNodes = paco2
})
      </div>
 
      <div class="rightColumn">
      </div>
 
    </div>
}
